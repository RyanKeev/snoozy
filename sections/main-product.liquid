<script>
  document.addEventListener('DOMContentLoaded', function() {
    const looxRatingElements = document.querySelectorAll('.loox-rating');
  
    looxRatingElements.forEach(element => {
      const raters = element.getAttribute('data-raters');
  
      // Remove parentheses if present
      if (raters.startsWith('(') && raters.endsWith(')')) {
        const formattedRaters = raters.replace('(', '').replace(')', '');
        element.setAttribute('data-raters', formattedRaters);
        element.textContent = formattedRaters; // Set the element's text to the formatted raters count
      }
    });
  });
</script>
<section id="MainProduct-{{ section.id }} {% if section.settings.use_product_template_style_2 %} ProductTemplate2 {% endif %}" class="section-{{ section.id }}-padding product-main-section" data-section="{{ section.id }}">
  <div class="container">
  {{ 'countdown.css' | asset_url | stylesheet_tag }}
  {{ 'section-main-product.css' | asset_url | stylesheet_tag }}
  {{ 'component-accordion.css' | asset_url | stylesheet_tag }}
  {{ 'component-price.css' | asset_url | stylesheet_tag }}
  {{ 'component-slider.css' | asset_url | stylesheet_tag }}
  {{ 'component-rating.css' | asset_url | stylesheet_tag }}
  {{ 'component-loading-overlay.css' | asset_url | stylesheet_tag }}
  {{ 'component-deferred-media.css' | asset_url | stylesheet_tag }}

